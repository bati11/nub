def inLength(s, acc) {
  if (isNil(s)) {
    return acc;
  } else {
    return inLength(cdr(s), acc + 1);
  }
}

def length(s) {
  return inLength(s, 0);
}

def indexOf(s, i) {
  if (i == 0) {
    return car(s);
  } else {
    indexOf(cdr(s), i - 1);
  }
}

let s = "Hello" + "World!";
println(indexOf(s, 5));
println(length(s));
